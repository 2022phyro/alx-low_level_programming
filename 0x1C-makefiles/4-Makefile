CC=gcc
SRC= main.c school.c
OBJ=main.o school.o
NAME=school
RM=rm -f
CFLAGS= -Wall -Werror -Wextra -pedantic
all: $(OBJ)
	$(CC) $(OBJ) -o $(NAME)
main.o: main.c
	$(CC) $(CFLAGS) -c -o main.o main.c
school.o: school.c
	$(CC) $(CFLAGS) -c -o school.o school.c
clean:
	$(RM) *~ *.swo *.swp school
oclean:
	$(RM) $(OBJ)
fclean:
	$(RM) *~ *.swo *.swp school
	$(RM) $(OBJ)
re:
	 $(CC) -c -o main.o main.c
	 $(CC) -c -o school.o school.c
	 $(CC) $(OBJ) -o $(NAME)
